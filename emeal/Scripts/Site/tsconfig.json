{
  "compilerOptions": {
    "outDir": "./dist/main.js",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "strict": true,
    "alwaysStrict": true,
    "allowJs": true,
    "checkJs": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "pretty": true,
    "target": "es5"
  },
  "lib": [
    "dom",
    "es5",
    "scripthost",
    "es2015.iterable"
  ],
  "include": [
    "./src/*.ts"
  ],
  "exclude": [
    "*.js",
    "dist",
    "node_modules",
    "wwwroot"
  ]
}